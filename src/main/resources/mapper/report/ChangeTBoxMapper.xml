<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitnei.cloud.report.mapper.ChangeTBoxMapper">

    <select id="findSimByIccid" parameterType="String" resultType="java.util.HashMap">
        SELECT
            *
        FROM
            sys_sim_management sm
        WHERE
            sm.iccid = #{value}
    </select>

    <select id="findTermUnitCountByIccid" parameterType="String" resultType="java.lang.Integer">
        SELECT
            count(0)
        FROM
            sys_term_model_unit
        WHERE
            iccid = #{value} and delete_flag = 0
    </select>

    <select id="findVehInfoByVin" parameterType="String" resultType="java.util.HashMap">
        SELECT
            *
        FROM
            sys_vehicle
        WHERE
            vin = #{value}
    </select>

</mapper>
