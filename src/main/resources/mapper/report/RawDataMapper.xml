        h.name as sPeople,
        h.create_time as sTime
        from
        raw_data rd
        LEFT JOIN hole h on rd.code=h.code
        <where>
            1=1
            <if test="code !=null and code  !=''">
                AND rd.code LIKE CONCAT('%',#{code},'%')
